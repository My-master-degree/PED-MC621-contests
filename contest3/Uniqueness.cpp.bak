#include<map>
#include<stdio.h>
#include<stdlib.h>
#include<iterator>
#include<iostream> 
#include<algorithm>

#define min(a,b) (a < b ? a : b)
#define max(a,b) (a > b ? a : b)
#define minmax(a, b) pair<int, int> (min(a, b), max(a, b))

using namespace std;

map<int, int>::iterator it;
int val;

bool mapStable(map<int, int>* m);

int main(){
	int n;
	scanf("%d", &n);	
	if (n >= 1 && n <= 2000){
		int a[n];
		int i, j, min = n;
		map<int, int> m, t;
		for (i = 0; i < n; i++){
			scanf("%d", &a[i]);	
			it = m.find(a[i]);			
			if (it != m.end()){
				it->second += 1;
			}else{
				m.insert(pair<int, int> (a[i], 1));
			}
		}
		if (!mapStable(&m)){
			for (i = 0; i < n; i++)	{
				t = m;
				for (j = i; j < n; j++){
					t.find(a[j])->second--;
					if (mapStable(&t) && j - i + 1 < min){
						min = j - i + 1;
					}
				}
			}
			printf("%d", min);
		}else{
			printf("0");
		}
		
	}
}

bool mapStable(map<int, int>* t){ 
	for (it = t->begin(); it != t->end(); ++it){
		if (it->second != 1){
			return false;				
		}
	}
	return true;
}
